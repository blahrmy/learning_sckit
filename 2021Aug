# You can write up to 20GB to the current directory (/kaggle/working/) that gets preserved as output when you create a version using "Save & Run All" 
# You can also write temporary files to /kaggle/temp/, but they won't be saved outside of the current session
/kaggle/input/tabular-playground-series-aug-2021/sample_submission.csv
/kaggle/input/tabular-playground-series-aug-2021/train.csv
/kaggle/input/tabular-playground-series-aug-2021/test.csv

import pandas as pd
import numpy as np
from copy import copy
import matplotlib.pyplot as plt
import matplotlib as mpl
%matplotlib inline

train = pd.read_csv("../input/tabular-playground-series-aug-2021/train.csv")
test = pd.read_csv("../input/tabular-playground-series-aug-2021/test.csv")
submission = pd.read_csv("../input/tabular-playground-series-aug-2021/sample_submission.csv")

from sklearn.model_selection import train_test_split
train, val = train_test_split(train, test_size=0.3)

target_col = 'loss'
#除外する列を選ぶ（不要な特徴量として除く）
exclude_cols = []
#説明変数が割当てられる。コード見るとターゲット列も入りそうだけど、入らないらしい。
feature_cols = []
for col in train.columns:
    if col not in exclude_cols:
        feature_cols.append(col)

X_train = train[feature_cols]
y_train = train[target_col]

X_val = val[feature_cols]
y_val = val[target_col]

#kaggleだとすでにtestからターゲット列は除外（submissionとして）されてる
#X_test = test[feature_cols]　　ここがtest
#y_test = test[target_col]　　　ここがsubmission

import xgboost as xgb
xg = xgb.XGBRegressor(random_state=1234)
xg.fit(X_train, y_train)
y_val_xgb_pred = xg.predict(X_val)

from sklearn.metrics import mean_squared_error
print('XGBoost'+'のバリデーションデータに対する評価')
print('RMSE=', np.sqrt(mean_squared_error(y_val, y_val_xgb_pred)))
val['xgb_pred'] = y_val_xgb_pred
val.plot(x='id', y=['loss', 'xgb_pred'])
plt.xticks(rotation=45)

import optuna
#optunaインストール前に実行できたから、ここはoptunaでなさそうだ
def objective_xgb(trial):
    xgb_param = {
        'n_estimators': trial.suggest_int('n_estimators', 1, 60),
        'max_depth': trial.suggest_int('max_depth', 1, 40),
        'min_child_weight': trial.suggest_int('min_child_weight', 1,  5),
        'subsample': trial.suggest_discrete_uniform('subsample', 0.5, 0.9, 0.1),
        'colsample_bytree': trial.suggest_discrete_uniform('colsample_bytree', 0.5, 0.9, 0.1),
    }
    xgb_param['random_state'] = 1234
    xgb_tuna = xgb.XGBRegressor(**xgb_param)
    xgb_tuna.fit(X_train, y_train)
    tuna_pred_val = xgb_tuna.predict(X_val)
    return np.sqrt(mean_squared_error(y_val, tuna_pred_val))
#optunaは40分以上かかるから保存の時は#しておかないと危険

#study_xgbという箱を用意して
#study_xgb = optuna.create_study()
#箱名.optimizeで実行するのかな？
#study_xgb.optimize(objective_xgb, n_trials=30)
#箱名.best_paramsで最適ハイパラが出るのか？

best_params_xgb = study_xgb.best_params
best_params_xgb

#optuna結果を手動でハイパラ入力する
xg_tuned = xgb.XGBRegressor(random_state=1234,
                            n_estimators=60,
                            max_depth=5,
                            min_child_weight=4,
                            subsample=0.9,
                            colsample_bytree=0.9
                            )
xg_tuned.fit(X_train, y_train)

XGBRegressor(base_score=0.5, booster='gbtree', colsample_bylevel=1,
             colsample_bynode=1, colsample_bytree=0.9, gamma=0, gpu_id=-1,
             importance_type='gain', interaction_constraints='',
             learning_rate=0.300000012, max_delta_step=0, max_depth=5,
             min_child_weight=4, missing=nan, monotone_constraints='()',
             n_estimators=60, n_jobs=4, num_parallel_tree=1, random_state=1234,
             reg_alpha=0, reg_lambda=1, scale_pos_weight=1, subsample=0.9,
             tree_method='exact', validate_parameters=1, verbosity=None)
             
# チューニング後のモデルを再度、バリデーションデータに適応させ、評価を確認
y_val_tuned_xgb_pred = xg_tuned.predict(X_val)

print('Tuned_XGBoost'+'のバリデーションデータに対する評価(チューニング後)')
print('RMSE=', np.sqrt(mean_squared_error(y_val, y_val_tuned_xgb_pred)))
Tuned_XGBoostのバリデーションデータに対する評価(チューニング後)

print('XGBoost'+'のバリデーションデータに対する評価')
print('RMSE=', np.sqrt(mean_squared_error(y_val, y_val_xgb_pred)))

print('Tuned_XGBoost'+'のバリデーションデータに対する評価(チューニング後)')
print('RMSE=', np.sqrt(mean_squared_error(y_val, y_val_tuned_xgb_pred)))

#結果的になぜかRMSE上がってしまってる。。。
今度はsubmission用をpredする
test['loss'] = submission['loss']
X_test = test[feature_cols]
y_test = test[target_col]
y_test.shape

xg = xgb.XGBRegressor(random_state=1234)
xg.fit(X_test, y_test)
test_xgb_pred = xg.predict(X_test)
test_xgb_pred

submission_data = pd.DataFrame(np.array([test_xgb_pred]).T,
                              columns=['loss'])

submission_data

#submission['loss'] = y_val_xgb_pred['loss']
submission['loss'] = submission_data['loss']
submission.to_csv('submission.csv', index=False)
 
